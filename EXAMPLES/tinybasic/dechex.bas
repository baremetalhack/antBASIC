0010 REM "Decimal to Hex"
0020 :
0030 PRINT "Decimal ";
0040 INPUT D
0050 IF D<=0 END
0060 IF D>255 GOTO 150
0070 H=D/16
0080 X=D-16*H
0090 PRINT "Hexadecimal is 0x";
0100 GOSUB 170
0110 IF H<=9 PRINT H;
0120 H=X
0130 GOSUB 170
0140 IF X<=9 PRINT X;
0150 PRINT:PRINT
0160 GOTO 30
0170 IF H==10 PRINT "A";
0180 IF H==11 PRINT "B";
0190 IF H==12 PRINT "C";
0200 IF H==13 PRINT "D";
0210 IF H==14 PRINT "E";
0220 IF H==15 PRINT "F";
0230 RETURN
